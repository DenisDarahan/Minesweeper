<CellButton>:
    background_normal: 'resources/cell_button_normal.png' if self.flag else 'resources/default_button_normal.png'
    background_down: 'resources/cell_button_down.png' if self.flag else 'resources/default_button_down.png'

    flag: 0

    size: '25dp', '25dp'

    on_long_press: self.change_flag_long_press()
    on_release: self.open()
